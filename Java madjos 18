import javax.swing.*;

public class etinjos {
    public static void main(String[] args) {
        int[] arr = {4, 2, 5, 7, 2, 9};
        boolean dup = false;

        for (int i = 0; i < arr.length; i++) {
            for (int j = i + 1; j < arr.length; j++) {
                if (arr[i] == arr[j]) {
                    dup = true;
                    break;
                }
            }
        }

        JOptionPane.showMessageDialog(null, "Contains duplicates? " + dup);
    }
}
