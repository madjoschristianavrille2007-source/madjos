import javax.swing.*;

public class Eatmadjos{
    public static void main(String[] args) {
        int[] A = {1, 2, 3, 4};
        int[] B = new int[A.length];

        for (int i = 0; i < A.length; i++) {
            B[i] = A[i];
        }

        JOptionPane.showMessageDialog(null, "Array B: " + java.util.Arrays.toString(B));
    }
}
