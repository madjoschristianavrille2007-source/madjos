import javax.swing.*;

public class Fortenjos {
    public static void main(String[] args) {
        int[] arr = {4, 1, 9, 2, 7};
        String result = "";

        for (int i = arr.length - 1; i >= 0; i--) {
            result += arr[i] + " ";
        }

        JOptionPane.showMessageDialog(null, "Reverse order:\n" + result);
    }
}
