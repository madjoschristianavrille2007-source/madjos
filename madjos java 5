public class madjoslima{
     public static void main (String [] args){
         int [] arr = {4,8,2,9,3};
         int target = 9;
         
         for(int i=0 ;i < arr.length; i++){
           if(arr[i] == target){
               System.out.println("Found at index "+ i);
               break;
           }
        }
    }
}
