import java.util.*;

public class madjosuna{
  public static void main(String [] args){
   Scanner sc = new Scanner(System.in);
   int num;
   
   do{
      System.out.println("Enter a number (1-100):");
      num = sc.nextInt();
    }while(num < 1 || num > 100);
    
    System.out.println("You entered: " + num);
    }
}
